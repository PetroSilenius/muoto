{
  "name": "app-playground",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "check-types": "tsc --noEmit",
    "postinstall": "prisma migrate deploy && prisma db seed"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "1.0.6",
    "@next-auth/prisma-adapter": "1.0.5",
    "@prisma/client": "4.5.0",
    "@tailwindcss/forms": "0.5.3",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "dinero.js": "2.0.0-alpha.8",
    "next": "13.0.2",
    "next-auth": "4.15.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "use-count-up": "3.0.1"
  },
  "devDependencies": {
    "@types/ms": "0.7.31",
    "@types/node": "18.11.6",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "autoprefixer": "^10.4.7",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "prisma": "4.5.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
